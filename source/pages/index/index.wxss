/**index.wxss**/
page {
  background-color: #eee;
}

.case-page {
  padding: 20rpx;
}

.list-masonry {
  column-count: 2;
  column-gap: 20rpx;
}

.item-masonry {
  background-color: #fff;
  break-inside: avoid; /*避免在元素内部插入分页符*/
  box-sizing: border-box;
  padding: 20rpx;
  margin-bottom: 20rpx;
}

.item-masonry image {
  width: 100%;
}

.goTop{
  height: 100rpx;
  width: 100rpx;
  position: fixed;
  bottom: 50rpx;
  right: 30rpx;
}
